#!/usr/bin/gnuplot -persist

#exporto como png
set term png

#le pone titulo al grafico
set title "Cantidad de Operaciones del algoritmo bn_mod_n"

#pone la descripccion de los ejes
set xlabel "n"
set ylabel "cantidad de operaciones"

#pone la leyenda a la izquierda y con borde
set key left box

#defino dos tipos de lineas
#lt=tipo de linea y color,  pt=point type,  ps=point size
#(para ver los distintos point types pongan "test" en el gnuplot)
set style line 1 lt 1 pt 1 ps 1
set style line 2 lt 2 pt 1 ps 1
set style line 3 lt 3 pt 1 ps 1

#dibuja los puntos. index es porque lo separe en bloques de primos/no primos para ponerle distinto estilo..
set output 'png/count_test.png'
plot[0:1e+09][0:500]	'data/count_test.dat' title "Cantidad de operaciones" linestyle 1,\
	19*log(x) title "log(n)*19" linestyle 2

set logscale x

set output 'png/count_test_if.png'
plot	'data/count_test_if.dat' title "Cantidad de operaciones" linestyle 1,\
	'data/count_test_no_if.dat' title "Cantidad de operaciones" linestyle 3,\
	19*log(x)+15 title "log(n)*19+15" linestyle 2

