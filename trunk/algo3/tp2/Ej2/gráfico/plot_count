#!/usr/bin/gnuplot -persist

#exporto como png
set term postscript enhanced color

#le pone titulo al grafico
set title "Cantidad de Operaciones del algoritmo ciudad"

#pone la descripccion de los ejes
set xlabel "cantidad de esquinas"
set ylabel "cantidad de operaciones"

#pone la leyenda a la izquierda y con borde
set key left box

#defino dos tipos de lineas
#lt=tipo de linea y color,  pt=point type,  ps=point size
#(para ver los distintos point types pongan "test" en el gnuplot)
set style line 1 lt 1 pt 1 ps 1

set output 'ps/count_completos.eps'
plot	'data/count_completos.dat' title "Completos",\
	'data/count_test_mejor_caso.dat' title "Mejor caso",\
	'data/count_test_random.dat' title "Random",\
	5.8*x*x*x title "5.8n^3"

set output 'ps/count_test_mejor_caso.eps'
plot[][0:3500]	'data/count_completos.dat' title "Completos",\
	'data/count_test_mejor_caso.dat' title "Mejor caso",\
	'data/count_test_random.dat' title "Random"
