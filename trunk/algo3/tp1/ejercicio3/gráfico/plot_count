#!/usr/bin/gnuplot -persist

#exporto como png
set term png

#le pone titulo al grafico
set title "Cantidad de Operaciones del algoritmo programadores_en_simultaneo"

#pone la descripccion de los ejes
set xlabel "cantidad de programadores"
set ylabel "cantidad de operaciones"

#pone la leyenda a la izquierda y con borde
set key left box

#defino dos tipos de lineas
#lt=tipo de linea y color,  pt=point type,  ps=point size
#(para ver los distintos point types pongan "test" en el gnuplot)
set style line 1 lt 1 pt 1 ps 1
set style line 2 lt 2 pt 1 ps 1 lw 2
set style line 3 lt 3 pt 1 ps 1
set style line 4 lt 7 pt 1 ps 1 lw 2

#dibuja los puntos. index es porque lo separe en bloques de primos/no primos para ponerle distinto estilo..
set output 'png/count_peor_mejor.png'
plot	'data/count_test_ing_eg.dat' title "Peor caso" linestyle 1,\
	'data/count_test_todosing_todoseg.dat' title "Mejor caso" linestyle 3,\
	13*x title "n*13" linestyle 2,\
	6*x title "n*6" linestyle 4

